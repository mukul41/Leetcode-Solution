class Solution {
public:
    int mySqrt(int x) {
        
        int ans;
        if (x==1)
{
    ans = 1;
    
}
else
{
    
    for (long long int i = 0; i <= x / 2; i++)
    {
        
        
        if ((i*i) <= x && (i+1) * (i+1) > x)
        {
            ans = i;
            break;
        }
        
    }
}
        return ans;

        
    }
};
