class Solution {
public:
    bool checkString(string s) {
        
        int count =0;
        int n =s.length();
        
        for(int i=1; i<s.length(); i++)
        {
            if(s[i-1]>s[i])
            {
                count++;
            }
        }
        
        return count<1;
    }
};
