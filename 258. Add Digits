class Solution {
    
public:
    int addDigits(int num) {
        
       while(num > 9)
        {
            int d1 = num%10;
            num /= 10;
            int d2 = num%10;
            num /= 10;
            
            num += d1 + d2;
        }
        
        return num;
      
        
        
    }
};
